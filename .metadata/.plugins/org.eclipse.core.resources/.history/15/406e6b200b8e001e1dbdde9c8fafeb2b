package pe.bn.com.modelo;
import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.Table;
import lombok.Data;

import java.io.Serializable;
import java.sql.Timestamp;

@Entity
@Data
@Table(name = "bnmssftf02_token_status",  schema = "bn_ms_soft")
public class Modelo_datos_token implements Serializable {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    @Column(name = "F02_ID_TOKEN_STATUS")
    private int F02_ID_TOKEN_STATUS;

    @Column(name = "F02_AFFIL_STATUS_FINAL", length = 255)
    private String F02_AFFIL_STATUS_FINAL;

    @Column(name = "F02_AFFIL_STATUS_INITIAL", length = 255)
    private String F02_AFFIL_STATUS_INITIAL;

    @Column(name = "F02_APPLICATION", length = 255)
    private String F02_APPLICATION;

    @Column(name = "F02_CHANNEL", length = 255)
    private String F02_CHANNEL;

    @Column(name = "F02_CHANNEL_CODE", length = 255)
    private String F02_CHANNEL_CODE;

    @Column(name = "F02_CIA_CODE", length = 255)
    private String F02_CIA_CODE;

    @Column(name = "F02_DATE_TOKEN_STATUS")
    private Timestamp F02_DATE_TOKEN_STATUS;

    @Column(name = "F02_EMAIL", length = 255)
    private String F02_EMAIL;

    @Column(name = "F02_ID_CLIENT", length = 255)
    private String F02_ID_CLIENT;

    @Column(name = "F02_MOBILE_STORE", length = 255)
    private String F02_MOBILE_STORE;

    @Column(name = "F02_MODEL_MOBILE", length = 255)
    private String F02_MODEL_MOBILE;

    @Column(name = "F02_NAME_CLIENT", length = 255)
    private String F02_NAME_CLIENT;

    @Column(name = "F02_NUMER_MOBILE", length = 9)
    private String F02_NUMER_MOBILE;

    @Column(name = "F02_OPER_MOBILE", length = 255)
    private String F02_OPER_MOBILE;

    @Column(name = "F02_REASON_CODE", length = 255)
    private String F02_REASON_CODE;

    @Column(name = "F02_SECURITY_STATUS", length = 255)
    private String F02_SECURITY_STATUS;

    @Column(name = "F02_TOKEN_NOTIFICATION", length = 255)
    private String F02_TOKEN_NOTIFICATION;

    @Column(name = "F02_TOKEN_STATUS_FINAL", length = 255)
    private String F02_TOKEN_STATUS_FINAL;

    @Column(name = "F02_TOKEN_STATUS_INITIAL", length = 255)
    private String F02_TOKEN_STATUS_INITIAL;

    @Column(name = "F02_TYPE_OPERATION", length = 255)
    private String F02_TYPE_OPERATION;


}

